{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "list_pages",
        "navigate_page",
        "take_snapshot",
        "take_screenshot",
        "list_network_requests",
        "list_console_messages",
        "evaluate_script",
        "get_network_request",
        "performance_start_trace",
        "new_page",
        "select_page",
        "click",
        "wait_for"
      ]
    }
  }
}